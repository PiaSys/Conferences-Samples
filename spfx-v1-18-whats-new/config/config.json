{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "flexible-ace-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/flexibleAce/FlexibleAceAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/flexibleAce/FlexibleAceAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "search-ace-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/searchAce/SearchAceAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/searchAce/SearchAceAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FlexibleAceAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/flexibleAce/loc/{locale}.js",
    "SearchAceAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/searchAce/loc/{locale}.js"
  }
}
