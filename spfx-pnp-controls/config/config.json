{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "play-with-taxonomy-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/playWithTaxonomy/PlayWithTaxonomyWebPart.js",
          "manifest": "./src/webparts/playWithTaxonomy/PlayWithTaxonomyWebPart.manifest.json"
        }
      ]
    },
    "play-with-people-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/playWithPeople/PlayWithPeopleWebPart.js",
          "manifest": "./src/webparts/playWithPeople/PlayWithPeopleWebPart.manifest.json"
        }
      ]
    },
    "play-with-list-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/playWithListView/PlayWithListViewWebPart.js",
          "manifest": "./src/webparts/playWithListView/PlayWithListViewWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PlayWithTaxonomyWebPartStrings": "lib/webparts/playWithTaxonomy/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PlayWithPeopleWebPartStrings": "lib/webparts/playWithPeople/loc/{locale}.js",
    "PlayWithListViewWebPartStrings": "lib/webparts/playWithListView/loc/{locale}.js"
  }
}
