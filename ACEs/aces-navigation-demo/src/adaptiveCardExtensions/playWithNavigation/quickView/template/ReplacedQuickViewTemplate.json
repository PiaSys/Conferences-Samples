{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.3",
  "body": [
      {
          "type": "Container",
          "items": [
              {
                  "type": "TextBlock",
                  "text": "This is the replaced QuickView!",
                  "wrap": true
              },
              {
                  "type": "Container",
                  "items": [
                      {
                          "type": "ColumnSet",
                          "columns": [
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "id": "goBackColumn",
                                  "items": [
                                      {
                                          "type": "Image",
                                          "url": "data:image/png;base64,",
                                          "horizontalAlignment": "Center",
                                          "selectAction": {
                                              "type": "Action.Submit",
                                              "title": "Go back",
                                              "id": "goBack",
                                              "data": ""
                                          }
                                      },
                                      {
                                          "type": "TextBlock",
                                          "wrap": true,
                                          "text": "Go Back",
                                          "horizontalAlignment": "Center"
                                      }
                                  ]
                              },
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "id": "finishColumn",
                                  "items": [
                                      {
                                          "type": "Image",
                                          "url": "data:image/png;base64,",
                                          "horizontalAlignment": "Center",
                                          "selectAction": {
                                              "type": "Action.Submit",
                                              "title": "Finish",
                                              "id": "finish",
                                              "data": ""
                                          }
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "Finish",
                                          "wrap": true,
                                          "horizontalAlignment": "Center"
                                      }
                                  ]
                              }
                          ],
                          "horizontalAlignment": "Center"
                      }
                  ]
              }
          ]
      }
 ]
}