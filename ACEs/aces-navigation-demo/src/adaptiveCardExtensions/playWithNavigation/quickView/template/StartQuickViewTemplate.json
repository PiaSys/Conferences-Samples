{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.3",
  "body": [
      {
          "type": "Container",
          "items": [
              {
                  "type": "TextBlock",
                  "text": "Choose what to do:",
                  "wrap": true
              },
              {
                  "type": "Container",
                  "items": [
                      {
                          "type": "ColumnSet",
                          "columns": [
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "id": "replaceColumn",
                                  "items": [
                                      {
                                          "type": "Image",
                                          "url": "data:image/png;base64,",
                                          "horizontalAlignment": "Center",
                                          "selectAction": {
                                              "type": "Action.Submit",
                                              "title": "Replace",
                                              "id": "replace",
                                              "data": ""
                                          }
                                      },
                                      {
                                          "type": "TextBlock",
                                          "wrap": true,
                                          "text": "Replace",
                                          "horizontalAlignment": "Center"
                                      }
                                  ]
                              },
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "id": "pushColumn",
                                  "items": [
                                      {
                                          "type": "Image",
                                          "url": "data:image/png;base64,",
                                          "horizontalAlignment": "Center",
                                          "selectAction": {
                                              "type": "Action.Submit",
                                              "title": "Push",
                                              "id": "push",
                                              "data": ""
                                          }
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "Push",
                                          "wrap": true,
                                          "horizontalAlignment": "Center"
                                      }
                                  ]
                              }
                          ],
                          "horizontalAlignment": "Center"
                      }
                  ]
              }
          ]
      }
 ]
}